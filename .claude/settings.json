{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "cleanupPeriodDays": 60,
  "permissions": {
    "allow": [
      "Bash(npm:*)",
      "Bash(git:*)",
      "Bash(cargo:*)",
      "Bash(find:*)",
      "Read(**)",
      "Edit(~/projects/**)",
      "WebFetch(domain:*)"
    ],
    "deny": [
      "Bash(sudo:*)",
      "Bash(rm:*)",
      "Bash(rm -rf:*)"
    ]
  },
  "enabledPlugins": {},
  "alwaysThinkingEnabled": true,
  "skipDangerousModePermissionPrompt": true,
  "effortLevel": "medium",
  "attribution": {
    "commit": "",
    "pr": ""
  }
}
